{
    "name": "react-simple-calendar-light",
    "version": "1.0.17",
    "private": false,
    "author": "Duuu",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uudeok/react-simple-calendar"
    },
    "keywords": [
        "react",
        "datepicker",
        "calendar",
        "month",
        "date"
    ],
    "type": "module",
    "main": "dist/index.umd.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs"
        },
        "./react-simple-calendar-light.css": "./dist/react-simple-calendar-light.css"
    },
    "peerDependencies": {
        "react": ">=18 <20",
        "react-dom": ">=18 <20"
    },
    "scripts": {
        "dev": "vite",
        "build": "rm -rf dist && vite build",
        "prepare": "npm run build",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "vitest",
        "publish:npm": "npm run test && npm run build && npm publish --access public"
    },
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.4.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vanilla-extract/css": "^1.17.4",
        "@vanilla-extract/dynamic": "^2.1.5",
        "@vanilla-extract/recipes": "^0.5.7",
        "@vanilla-extract/vite-plugin": "^5.1.1",
        "@vitejs/plugin-react": "^4.6.0",
        "eslint": "^9.30.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "jsdom": "^27.0.0",
        "path": "^0.12.7",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.35.1",
        "vite": "^7.0.4",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4"
    }
}
